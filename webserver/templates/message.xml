<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/static/message.xsl"?>
<message>
    <envelope>
        <sender>{{msg['sender']}}</sender>
        <receiver>{{msg['receiver']}}</receiver>
        <time>{{msg['time']}}</time>
    </envelope>
    <content>
        <text>{{msg['content']['text']}}</text>
    </content>
    <data>{{msg['data']}}</data>
    <signature>{{msg['signature']}}</signature>
</message>
